	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + (active ? 1231 : 1237);
		result = prime * result
				+ ((answer_1 == null) ? 0 : answer_1.hashCode());
		result = prime * result
				+ ((answer_10 == null) ? 0 : answer_10.hashCode());
		result = prime * result + answer_10_count;
		result = prime * result + answer_1_count;
		result = prime * result
				+ ((answer_2 == null) ? 0 : answer_2.hashCode());
