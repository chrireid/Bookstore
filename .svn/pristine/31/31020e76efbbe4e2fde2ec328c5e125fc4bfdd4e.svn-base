		result = prime * result
				+ ((answer_9 == null) ? 0 : answer_9.hashCode());
		result = prime * result + answer_9_count;
		result = prime * result + (int) (id ^ (id >>> 32));
		result = prime * result
				+ ((question == null) ? 0 : question.hashCode());
		return result;
	}

